{
  "name": "@whyking/viking-tool",
  "description": "These stuff are my personal CLI tools.",
  "version": "1.0.0-milestone.2",
  "author": "whyking (https://github.com/whyking/)",
  "bin": {
    "viking-tool": "./bin/run"
  },
  "bugs": "https://github.com/whyking/viking-tool/issues",
  "dependencies": {
